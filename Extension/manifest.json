{
  "manifest_version": 2,
  "name": "SDS",
  "description": "Extensi√≥n para la asignatura de SDS",
  "version": "1",
  "author": "",
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_title": "SDS",
    "default_popup": "newtab.html"
